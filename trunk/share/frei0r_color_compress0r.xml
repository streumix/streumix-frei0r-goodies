<!DOCTYPE kpartgui>
<effect tag="frei0r.color_compress0r" id="frei0r.color_compress0r">
	<name>Color Compress0r</name>
	<description>Color curves compression based on power function</description>
	<author>Toby Mangold</author>
	<parameter type="constant" name="x-threshold" default="500" min="0" max="1000" factor="1000">
		<name>Threshold (x)</name>
	</parameter>
	<parameter type="constant" name="y-threshold" default="500" min="0" max="1000" factor="1000">
		<name>Threshold level (y)</name>
	</parameter>
	<parameter type="constant" name="slope" default="450" min="0" max="900" factor="900">
		<name> Slope @ Threshold</name>
	</parameter>
	<parameter type="bool" name="hsv" default="0">
		<name>HSV color model</name>
	<parameter type="bool" name="channel_0" default="1">
		<name>Map channel B/V</name>
	</parameter>
	<parameter type="bool" name="channel_1" default="1">
		<name>Map channel G/S</name>
	</parameter>
	<parameter type="bool" name="channel_2" default="1">
		<name>Map channel R/H</name>
	</parameter>
	<parameter type="bool" name="channel_3" default="0">
		<name>Map alpha</name>
	</parameter>
	<parameter type="bool" name="histogram" default="0">
		<name>Overlay histogram / mapping curve</name>
	</parameter>
	<parameter type="constant" name="hsize" default="50" min="0" max="100" factor="100">
		<name>Graph size</name>
	</parameter>
	<parameter type="constant" name="hposition_x" default="50" min="0" max="100" factor="100">
		<name>Graph position x</name>
	</parameter>
	<parameter type="constant" name="hposition_y" default="95" min="0" max="100" factor="100">
		<name>Graph position y</name>
	</parameter>
</effect>
